{
  "name": "mindmap",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/mindmap",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "packages/mindmap",
        "outputPath": "dist/packages/mindmap"
      },
      "configurations": {
        "production": {},
        "development": {
          "outputPath": "packages/mindmap"
        }
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "mindmap:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "mindmap:build:production",
          "dev": false
        },
        "development": {
          "buildTarget": "mindmap:build:development",
          "dev": true
        }
      },
      "defaultConfiguration": "development"
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "mindmap:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/mindmap"],
      "options": {
        "jestConfig": "packages/mindmap/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/mindmap/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
