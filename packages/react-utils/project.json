{
  "name": "react-utils",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/react-utils/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/react-utils",
        "main": "packages/react-utils/src/index.ts",
        "tsConfig": "packages/react-utils/tsconfig.lib.json",
        "assets": ["packages/react-utils/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/react-utils/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/react-utils"],
      "options": {
        "jestConfig": "packages/react-utils/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
