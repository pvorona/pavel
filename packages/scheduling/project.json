{
  "name": "scheduling",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/scheduling/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/scheduling",
        "main": "packages/scheduling/src/index.ts",
        "tsConfig": "packages/scheduling/tsconfig.lib.json",
        "assets": ["packages/scheduling/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/scheduling/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/scheduling"],
      "options": {
        "jestConfig": "packages/scheduling/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
