{
  "root": "packages/data-structures",
  "sourceRoot": "packages/data-structures/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/data-structures",
        "main": "packages/data-structures/src/index.ts",
        "tsConfig": "packages/data-structures/tsconfig.lib.json",
        "assets": ["packages/data-structures/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/data-structures/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/data-structures"],
      "options": {
        "jestConfig": "packages/data-structures/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
