{
  "root": "packages/request-idle-callback",
  "sourceRoot": "packages/request-idle-callback/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/request-idle-callback",
        "main": "packages/request-idle-callback/src/index.ts",
        "tsConfig": "packages/request-idle-callback/tsconfig.lib.json",
        "assets": ["packages/request-idle-callback/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/request-idle-callback/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/request-idle-callback"],
      "options": {
        "jestConfig": "packages/request-idle-callback/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
