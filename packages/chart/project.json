{
  "name": "chart",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/chart/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/chart",
        "main": "packages/chart/src/index.ts",
        "tsConfig": "packages/chart/tsconfig.lib.json",
        "assets": ["packages/chart/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/chart/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/chart"],
      "options": {
        "jestConfig": "packages/chart/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
