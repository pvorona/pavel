generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(cuid())
}

model Game {
  id     String     @id @default(cuid())
  status GameStatus
}

enum GameStatus {
  WAITING_FOR_OPPONENT
  IN_PROGRESS
  ENDED
}
