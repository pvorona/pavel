{
  "name": "chart-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/chart-demo/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/chart-demo",
        "compiler": "babel",
        "index": "packages/chart-demo/src/index.html",
        "baseHref": "/",
        "main": "packages/chart-demo/src/main.ts",
        "polyfills": "packages/chart-demo/src/polyfills.ts",
        "tsConfig": "packages/chart-demo/tsconfig.app.json",
        "assets": [
          "packages/chart-demo/src/favicon.ico",
          "packages/chart-demo/src/assets"
        ],
        "styles": ["packages/chart-demo/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "packages/chart-demo/src/environments/environment.ts",
              "with": "packages/chart-demo/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "chart-demo:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "chart-demo:build:production"
        },
        "development": {
          "buildTarget": "chart-demo:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/chart-demo/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/chart-demo"],
      "options": {
        "jestConfig": "packages/chart-demo/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
