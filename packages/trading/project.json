{
  "name": "trading",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/trading/src",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/trading",
        "main": "packages/trading/src/index.ts",
        "tsConfig": "packages/trading/tsconfig.lib.json",
        "assets": ["packages/trading/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/trading/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/trading"],
      "options": {
        "jestConfig": "packages/trading/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
