{
  "name": "redux-slice",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/redux-slice/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/redux-slice",
        "main": "packages/redux-slice/src/index.ts",
        "tsConfig": "packages/redux-slice/tsconfig.lib.json",
        "assets": ["packages/redux-slice/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/redux-slice/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/redux-slice"],
      "options": {
        "jestConfig": "packages/redux-slice/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
